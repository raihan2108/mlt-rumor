  <script>
    var CIL_config = {
      ajaxUrl: "http://www.welt.de/wirtschaft/article138724152/Kann-Auswertung-der-Handys-Absturzursache-klaeren.html?ajax&wid=128562862&aid=138725299&view=json&ct=cil",
      liveTickerSelector: ".widget.liveticker"
