<script type="text/javascript">  
(function(){


if(location.href.match('/regionales/hamburg/') || location.href.match('/regionales/berlin/')){
createOutbrainWidget('outbrainDiv','AR_3','weltde' );
}else{
createOutbrainWidget('outbrainDiv','AR_1','weltde' );
createOutbrainWidget('outbrainDiv2','AR_2','weltde' );
}


function createOutbrainWidget(divID, outbrainID, template){
    var oB = document.getElementById(divID); // update Outbrain Div dynamically
	oB.setAttribute("class", "OUTBRAIN");
	oB.setAttribute("data-ob-template", template);
	oB.setAttribute("data-widget-id", outbrainID);
	oB.setAttribute("data-src", (function(){var arrLink=document.getElementsByTagName("link");for(var i=0;i<arrLink.length;i++)if((arrLink[i].rel||"").toLowerCase()==="canonical")return arrLink[i].href||"";return""})() );
	 
	var oS = document.createElement('script'); // load Outbrain Javascript dynamically
	oS.type = 'text/javascript';
	oS.async = true;  
	oS.src = 'http://widgets.outbrain.com/outbrain.js';
	var s = document.getElementsByTagName('script')[0];
	s.parentNode.insertBefore(oS, s);
}

})();
</script></div>
<script type="text/javascript">
    var tokenFound = false;
    if(window.location.href.indexOf("?") != -1) {
        var param = window.location.href.split("?")[1];
        if(param.indexOf("token=") != -1) {
            tokenFound = true;
        }
    }

    if(tokenFound) {
        $.cookie('ASsocialOptout', "true",{domain:'welt.de',path:'/'})
    } else {
        $.cookie('ASsocialOptout', null,{domain:'welt.de',path:'/'})
    }
