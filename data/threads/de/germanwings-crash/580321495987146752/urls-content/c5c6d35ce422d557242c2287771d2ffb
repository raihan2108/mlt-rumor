<script type="text/javascript">
  (function($) {
    var self = {
      $activators: $('#dsqButton, .disqus.articleComments .header, #viewport.article .commentCount, #viewport.article .socMedArtComment a'),
      $dsqContainer: $('#disqus_thread'),
      $adContainer: $('#disqusPerformanceAd'),
      dsqOffset: 0,
      adOffset: 0,
      animate: 0,
      initAd: function(e) {
        var self = e.data;
        if (self.$dsqContainer.size() === 0 || self.$adContainer.size() === 0) return false;
        if (self.$adContainer.hasClass('hidden') && self.$dsqContainer.height() > 200) {
          self.$adContainer.removeClass('hidden');
          self.dsqOffset = self.$dsqContainer.offset().top;
          self.adOffset = self.$adContainer.offset().top;
          self.positionAd(e);
          self.animate = 100;
          $(window).on('scroll', self, self.positionAd);
        }
      },
      positionAd: function(e) {
        var self = e.data;
        
        var minMargin = Math.max(self.dsqOffset - 98, $(window).scrollTop() + 10) - self.adOffset;
        var maxMargin = self.dsqOffset + self.$dsqContainer.height() - self.adOffset - 340;
        var usedMargin = Math.max(0, Math.min(minMargin, maxMargin));

self.$adContainer.clearQueue().delay(self.animate).animate({marginTop: usedMargin}, self.animate*2);
      }
    };
    self.$activators.one('click', self, self.initAd);
  })(jQuery);
</script>
