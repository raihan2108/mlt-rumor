    $(document).ready(function () {
        var newsTicker = $('.newsticker').find('.scrollingtext');
        if (newsTicker != undefined && newsTicker.length > 0) {
            newsTicker.each(function() {
                $(this).text($(this).text().trim());
            });
        }
    });
</script>



                        
                        
                            
