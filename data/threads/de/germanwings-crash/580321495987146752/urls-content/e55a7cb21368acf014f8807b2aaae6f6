    <!-- Start Global Search component -->
    

<script type="text/javascript">
    $(document).ready(function() {
        $('#globalsearch').change(function() {
            validateSearchBox();
        });
    });

    function GlobaleSucheKampagne(e) {
        cc_attributes["etcc_med"] = "suche";
        cc_attributes["etcc_grp"] = "Suche";
        cc_attributes["etcc_ctv"] = "Globale Suche";
        cc_attributes["etcc_var"] = "Suchfeld";
        cc_attributes["etcc_cu"] = "onsite";
        cc_attributes["etcc_cmp"] = "Suche+mit+Ergebnis";
        cc_attributes["dus_searchword"] = '';

        var et_pagename = "[DE] Search - Globale Suche vir.";
        et_eC_Wrapper('dCm5hb', "[DE] Search - Globale Suche vir.", "DUS/[DE]/Home/404", '', '', "Globale Suche", '', '', '', '', '', '', '', '');
        et_cc_wrapper('3iyXh33AlpwV0JoMfck65BdCANdXadXySIuhu1ix9Bw=', '{cc_pagename:' + "[DE] Search - Globale Suche vir." + '}');
        redirectToSearch();
    }

    function redirectToSearch() {
        if (validateSearchBox()) {
            var searchString = $('#globalsearch').val();
            var url = $('#searchResultPageUrl').val();
            var redirectUrl = url + "?" + "page" + "=1&" + "t" + "=1&" + "c" + "=1&" + "s" + "=" + searchString;
            window.location.href = redirectUrl;
        }
    }
    
    function validateSearchBox() {
        if (new RegExp("^[^<>%$~*^#]*$").test($('#globalsearch').val())) {
            $('#globalSearchInvalidCharacterError').hide();
            return true;
        } else {
            $('#globalSearchInvalidCharacterError').show();
            return false;
        }
    }
</script>

<!-- Start Global Search component -->
<script defer src="/static/js/selflearningsearch.js"></script>
<div class="grid widget search" role="search">
	
    <input type="hidden" id="searchResultPageUrl" value="/de-de/suche" />
    <fieldset>
        <legend>Inhalte durchsuchen</legend>
        <label class="invisible" for="global-search">
            Suche
        </label>
        <div class="col autocomplete">
            <input name="phheader_0$ctl03$phheaderflightsearch_0$globalsearch" type="search" maxlength="40" id="globalsearch" class="ui-input placeholder self-learning-search-autocomplete" placeholder="Flugnr./Suche" data-source="/de-de/global/ajax/selflearning/ajaxautosuggestselflearning" />
        </div>
        <input type="submit" name="phheader_0$ctl03$phheaderflightsearch_0$globalsearchButton" value="" onclick="GlobaleSucheKampagne();return false;" id="globalsearchButton" class="ui-submit ui-device-submit" formnovalidate="formnovalidate" />
    </fieldset>
    <span id="globalSearchInvalidCharacterError" style="display: none;" class="validation maxlengthregularexpressionvalidator">
        <span>
            Nicht alle Zeichen sind erlaubt
        </span>
    </span>

</div>

    <!-- End Global Search component -->

