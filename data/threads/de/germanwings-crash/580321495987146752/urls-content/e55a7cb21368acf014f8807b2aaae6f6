<script type="text/javascript">
    function eTrackerNewsletterTextBoxClicked() {
        var et_target = "Newsletter_Widget";
        et_eC_Wrapper("dCm5hb", "[DE] Error 404/Home/404 -26405D51D2F640AFA59E3DD3B34387A5- Newsletter_Widget vir.", 'DUS/[DE]', '', '', "Newsletter_Widget", '', '', '', '', '', '', '', '', '');
        et_cc_wrapper("3iyXh33AlpwV0JoMfck65BdCANdXadXySIuhu1ix9Bw=", '{cc_pagename:' + "[DE] Error 404/Home/404 -26405D51D2F640AFA59E3DD3B34387A5- Newsletter_Widget vir." + '}');
    };

    function eTrackerNewsletterButtonClicked() {
        var et_target = "Newsletter_Widget/Abschicken";
        et_eC_Wrapper("dCm5hb", "[DE] Error 404/Home/404 -26405D51D2F640AFA59E3DD3B34387A5- Newsletter_Widget - Bestätigung vir.", 'DUS/[DE]', '', '', "Newsletter_Widget/Abschicken", '', '', '', '', '', '', '', '', '');
        et_cc_wrapper("3iyXh33AlpwV0JoMfck65BdCANdXadXySIuhu1ix9Bw=", '{cc_pagename:' + "[DE] Error 404/Home/404 -26405D51D2F640AFA59E3DD3B34387A5- Newsletter_Widget - Bestätigung vir." + '}');
        
        var link = "/de-de/services/newsletter" + "?e=" + $('#dusnewsletter').val();
        window.location.href = link;
    };
</script>



