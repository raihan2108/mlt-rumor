<script>
	var funke_fktl = funke_fktl
			|| (function() {

				var visible1 = false;
				var visible2 = false;

				function init() {
				}

				function isDigit(aChar) {
					myCharCode = aChar.charCodeAt(0);

					if ((myCharCode > 47) && (myCharCode < 58)) {
						return true;
					}

					return false;
				}

				function reduceUrl(mode) {
					var s = window.location.href;
					var j = s.indexOf('?');
					if (j >= 0) {
						s = s.substring(0, j);
					}
					if (mode == 2)
						return s;
					if (s.substring(s.length - 5) == '.html') {
						var i;
						for (i = s.length - 6; i > 0; --i) {
							var c = s.substring(i, i + 1);
							if (!isDigit(c))
								break;
						}
						var d = s.substring(i + 1, s.length - 5);
						if (d.length > 0) {
							var a = '';
							if (s.indexOf('/nrz') >= 0) {
								a = 'nrz/';
							}
							if (s.indexOf('/wp') >= 0) {
								a = 'wp/';
							}
							if (s.indexOf('/wr') >= 0) {
								a = 'wr/';
							}
							if (s.indexOf('/ikz') >= 0) {
								a = 'ikz/';
							}

							return 'http://www.derwesten.de/' + a + d;
						}

					}
					return s;
				}

				function unstick() {
					jQuery(".social_popup_1").css(
							"left",
							(jQuery(".GridSticky").position().left - 320)
									+ "px");
					jQuery(".social_popup_1").css(
							"top",
							(jQuery(".GridSticky").position().top + jQuery(
									".GridSticky").height())
									+ "px");
					jQuery(".social_popup_2").css(
							"left",
							(jQuery(".GridSticky").position().left + 95)
									+ "px");
					jQuery(".social_popup_2").css(
							"top",
							(jQuery(".GridSticky").position().top + jQuery(
									".GridSticky").height())
									+ "px");
				}

				function stick() {
					jQuery(".social_popup_1").css("left", "-320px");
					jQuery(".social_popup_1").css("top",
							jQuery(".GridSticky").height() + "px");
					jQuery(".social_popup_2").css("left", "95px");
					jQuery(".social_popup_2").css("top",
							jQuery(".GridSticky").height() + "px");
				}

				function toggle1(){
					if(visible2){
						toggle2();
					}
					if (visible1) {
						jQuery(".social_popup_1").slideUp("fast");

					} else {
						jQuery(".social_popup_1").hide();
						jQuery(".social_popup_1").css("opacity", "100");
						jQuery(".social_popup_1").css("overflow", "visible");
						jQuery(".social_popup_1").css("height", "100px");

						jQuery(".social_popup_1").slideDown("fast");
					}
					visible1 = !visible1;
				}

				function toggle2(){
					if(visible1){
						toggle1();
					}
					if (visible2) {
						jQuery(".social_popup_2").slideUp("fast");

					} else {
						jQuery(".social_popup_2").hide();
						jQuery(".social_popup_2").css("opacity", "100");
						jQuery(".social_popup_2").css("overflow", "visible");
						jQuery(".social_popup_2").css("height", "100px");
						jQuery(".social_popup_2").slideDown("fast");
					}
					visible2 = !visible2;
				}
				
				function click(idx) {
					switch(idx){
					case 3:
						toggle2();
						break;
					case 4:
						var h = window.location.href;
						if( h.indexOf('?')  >= 0 ) {
							h += '&';
						}else{
							h += '?';
						}
						h += 'sendarticle=true&t=nachricht';
						window.location.href = h;
						return false;
					case 5:
						jQuery('html, body').animate({
					        scrollTop: $("#comments").offset().top - 60
					    }, 2000);
						break;
					case 6:
						window.open('http://www.derwesten.de/widget/id6005744?ctxArt=' + wnm_article_id + '&view=print', 'DerWestenDruckAnsicht', 'width=701,height=400,status=yes,scrollbars=yes,resizable=no');
						break;
						default:
							toggle1();							
					}
				}

				function click2(idx) {
					switch(idx){
						case 1:
							resize(-1);
							break;
						default:
							resize(+1);
							break;
					}
				}

				function resize( value ) {
					$text = $(".article > *, .content > *").not(".art_aside").not(".content");
					$text
						.each(function() {
							var fontSize = parseInt($(this).css("font-size")) + value + "px",
								lineHeight = parseInt($(this).css("line-height")) + value + "px";

							$(this)
								.css({
									"font-size": fontSize,
									"line-height": lineHeight
								});
						});

				};
				
				init();

				return {
					click : click,
					click2 : click2,
					unstick : unstick,
					stick : stick,
					reduceUrl : reduceUrl
				}

			})();
</script>


