   document.write('<script type="text/javascript" src="http://ad.de.doubleclick.net/adj/iqwiwo/unternehmen/dienstleister/art/;kw=iqwiwo,unternehmen,US29081M1027,DE0008232125;doc=artikel;sz=300x250,300x180,300x100;tile=9;tma=GermanwingsUnglück;' + n_pbt + ';ord=' + ord + '?"><\/script>');
   //]]>
</script>
