            <div id="DMN-analytics-data"></div>
    <script type="text/javascript">
        
        var s_config = {};
        s_config.account_id = "dmtfronlineprod";
        s_config.linkInternalFilters = ".fr-online.de";

        
        var DMN = {};
        DMN.analytics = (function(){
            'use strict';
            var trackingJSON = {};
            trackingJSON.pageName = 'Kultur.Dossier.Raubkunst.Kunstmuseum will Gurlitt-Erbe annehmen'.replace(/[,;|]+?/ig, "_");
            trackingJSON.site = 'http://www.fr-online.de';
            trackingJSON.ressort = 'Kultur';
            trackingJSON.category = 'Kultur_Dossier_Raubkunst_';
            trackingJSON.adZone = 'entertainment';
            trackingJSON.pageType = 'Artikelseiten';
            trackingJSON.channel = 'Online';
            trackingJSON.dateUnix = '23785994'; 
            trackingJSON.ivwZone = 'fr_kul';
            trackingJSON.articleId = '29114124';
            trackingJSON.articleTitle = 'Kunstmuseum will Gurlitt-Erbe annehmen'.replace(/[,;|]+?/ig,"_");
            trackingJSON.datePublishedUnix = '23609374';
            trackingJSON.articleViewsVisit = '+1';

            var setEvent = function (event, clearValues){
                if (clearValues || !trackingJSON.event) {
                    trackingJSON.event = event;
                } else if (trackingJSON.event.indexOf(event) === -1) {
                        trackingJSON.event += ',' + event;
                }
            };

            return {

                trackTeaser: function (contentID, contentView){
                    setEvent('event21', false);
                    if (!trackingJSON.list1){
                        trackingJSON.list1 = contentID + '#' + contentView;
                    } else {
                        trackingJSON.list1 += '|' + contentID + '#' + contentView;
                    }
                },

                trackTeaserClick: function (teaserID, teaserView) {
                    _satellite.setVar('teaserID_click', teaserID + '#' + teaserView);
                },

                trackImageGallery: function (galleryID, galleryPosition, gallerySize, galleryName) {
                    trackingJSON.picGalery = galleryName;
                    trackingJSON.picGaleryElement = galleryID + ' - ' + galleryPosition + '/' + gallerySize;
                    _satellite.track('picView');
                    this.generateJSON();
                },

                trackTextGallery: function (textGalleryID, textGalleryPosition, textGallerySize) {
                    trackingJSON.textGalery = textGalleryID;
                    trackingJSON.textGaleryElement = textGalleryID + ' - ' + textGalleryPosition + '/' + textGallerySize;
                    _satellite.track('textlineView');
                    this.generateJSON();
                },

                trackSnackTV : function (videoID) {
                    trackingJSON.videoID = videoID;
                },

                trackShortContentView : function (contentTeaser) {
                    trackingJSON.contentViewType = contentTeaser;
                },
                checkContentViewType : function(){
                    if(trackingJSON.contentViewType !== null && trackingJSON.contentViewType !== 'Kurzansicht'){
                        trackingJSON.contentViewType = 'Vollansicht';
                    }
                },
                generateJSON: function () {
                    var w = document.getElementById("DMN-analytics-data");
                    w.innerHTML = '';
                    var s = document.createElement("script");
                    s.type = 'text/javascript';
                    s.text = 'dataLayer = ' + JSON.stringify(trackingJSON, null, ' ');
                    w.appendChild(s);
                }
            };
        }());
        
        DMN.analytics.generateJSON();
    </script>
