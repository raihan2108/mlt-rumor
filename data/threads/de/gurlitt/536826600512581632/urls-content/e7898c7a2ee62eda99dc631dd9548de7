<script type="text/javascript">
    // Example for testing:
    // var advertisements = new Array('leaderboard', 'rectangle');

    function hideAdvertisement(advertisements){

        // check if 'advertisement' if of type Array()
        if( (typeof(advertisements) == "object") && (advertisements instanceof Array) ){
            for (var i = 0; i < advertisements.length; ++i){

                switch(advertisements[i]) {
                    case 'leaderboard':
                        $(document).ready(function() {
                            $('#All').addClass('noBanner');
                        });

                        break;

                    case 'rectangle':
                        // hide rectangle
                        break;

                    default:
                        console.log('default check!');
                }

            }
        }

    }
</script><script type="text/javascript">



var fixable = true;
var BrowserModus = document.compatMode;
var BrowserTyp = navigator.userAgent;




function sdibgclick(sdibgclickurl){

var div = document.createElement('div');
div.id = 'background-click-layer';

if (document.body.firstChild)
document.body.insertBefore(div, document.body.firstChild);
else
document.body.appendChild(div);

document.getElementById('background-click-layer').innerHTML += '<a href="'+sdibgclickurl+'" target="_blank" rel="nofollow" id="background-click-layer" width="100%" height="100%" border="0"></a>';
}


if (BrowserModus == "BackCompat" && BrowserTyp.indexOf("MSIE") != -1) fixable = false;


if (fixable == true){
    document.write('<style type="text/css">#background-click-layer {display:block; position:fixed;}</style>');
}
