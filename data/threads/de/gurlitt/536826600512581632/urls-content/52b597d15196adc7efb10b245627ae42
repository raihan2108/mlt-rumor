    <script>
    function initCXense() {
        var loginState = "unknown";
        
        if(window.loginState == 0) {
            loginState = "not-logged-in";
        } else if(window.loginState == 1) {
            loginState = "logged-in";
        }

        TAMSiteId = "9222261986533147877"; 
        cxParamD ='[{"tam-pagetype", "article"}'+
        ',{ "tam-target", "desktop_site"}'+
        ',{ "tam-loggedin-state", "'+loginState+'"}'+
        ',{ "tam-category", "kultur"}'+
        ',{ "tam-subcategory", "kunst"}'+
        ',{ "tam-slideshow", ""}'+
        ',{ "tam-slideshowstate", ""}'+
        ',{ "tam-video", ""}'+
        ',{ "tam-videostate", ""}'+
        ',{ "tam-cmssystem", "nn1"}' +
        ',{ "tam-article", "27435237"}]';

        triggerCxPageTracking();
    }
    </script>
    

