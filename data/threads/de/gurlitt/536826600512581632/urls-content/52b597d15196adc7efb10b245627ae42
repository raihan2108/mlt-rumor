<script>
window.loginState = 2;
var tmpRequest = $.ajax({
    type: "HEAD",
    async: true,
    cache: false,
    url: 'http://www.tagesanzeiger.ch/extern/payment.tmpl'+'?1='+Math.random(),
    beforeSend: function(xhr) {
        return xhr.setRequestHeader("X-Cache-Control", "no-cache");
    },
    success: handleResponse,
    error: function() { initCXense(); },
});

function handleResponse(message) {
    var userInfo = $.parseJSON(tmpRequest.getResponseHeader("C1-User-Info"));
    if (window.location.toString().indexOf("devlocal") > 1) {
        userInfo = {first_name : 'Andreas', last_name : 'Müller', title : 'Herr', login : 'andreas.mueller@tamedia.ch'};
    }

    if(userInfo!=null) {
        window.loginState = 1;
        if (userInfo.last_name !== '' && userInfo.last_name !== '-') {
            $('#name_button_paywall').text('Grüezi, '+userInfo.title +' '+ userInfo.first_name + ' ' + userInfo.last_name);
        } else {
            $('#name_button_paywall').text('Grüezi, '+userInfo.login);
        }
        
        $(document).ready(function() {
            $('#logout_button_paywall').show();
            $('#abo_button_paywall').hide();
            $('#login_button_paywall').hide();
            $('#name_button_paywall').show();
            $('#myabo_button_paywall').show();
            $('#logout_button_paywall').show();
        });
    } else {
        window.loginState = 0;
        $(document).ready(function() { 
            $('#abo_button_paywall').show();
            $('#login_button_paywall').show();
            $('#name_button_paywall').hide();
            $('#myabo_button_paywall').hide();
            $('#logout_button_paywall').hide();
        });
    }

    initCXense();
}
</script>
    
    
