<script defer src="http://track.tagesanzeiger.ch/cre-1.0/static/tracking/1.0/templateclient.js"></script>
<script defer src="http://track.tagesanzeiger.ch/cre-1.0/cockpit/platform/tamedia/shop/tagi/api/templatesets.js"></script>
<script defer src="http://track.tagesanzeiger.ch/cre-1.0/tracking/tracking.js"></script>

<script>
	var checkCreClient = window.setInterval(function(){
		if(window.cre_templateclient && window.cre_templatesets){
			startCreClient();
			window.clearInterval(checkCreClient);
		}

	}, 1000) 

	function startCreClient() {
		cre_templateclient.setup();

		var url, regexResult, channels, cmsId = "", doctype = "section";
		url = window.location.href.toString().split(window.location.host)[1];
		regexResult = url.match(/.*\/story\/([0-9]{8})/i);
	
		if(regexResult && regexResult.length > 1) {
			cmsId = regexResult[1];
			doctype = "article";
		}  
	
		cre_client.set_page_view();
		cre_client.set_origin("web");
		cre_client.set_cms_id(cmsId);
		cre_client.set_content_id(url);
		cre_client.set_service_id('tagesanzeiger');
		
		channels = url.split("/");	
		cre_client.set_channel(window.location.pathname === "/" ? "home" : channels[1]);

		if(channels[2]) {
			cre_client.set_sub_channel(channels[2]);
		}

		cre_client.set_heading($("title").text());
		cre_client.set_doc_type(doctype);
		cre_client.request();
	}

</script>

