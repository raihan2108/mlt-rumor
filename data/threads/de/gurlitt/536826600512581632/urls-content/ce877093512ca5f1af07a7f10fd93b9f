                    });

                    
                    var cacEnableAdHandler = function () {
                        $(document).ready(function () {
                            $(".cacDisabled").css('display', 'block');
                        });
                    };

                    var cacDisableAdHandler = function () {
                        $(document).ready(function () {
                            $(".cacDisabled").remove();
                        });
