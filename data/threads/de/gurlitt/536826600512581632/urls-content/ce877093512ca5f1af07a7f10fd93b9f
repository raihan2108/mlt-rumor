            var removeOmnitureParameters = function () {
                var rcStr = [];
                var deprList = ['prop6', 'prop8', 'prop9',
                    'prop11', 'eVar6', 'eVar16'];
                var propertyName;
                var eventsSplitted;

                if (typeof s == 'object') {
                    for (var deprItem in deprList) {
                        propertyName = deprList[deprItem];
                        if(s.hasOwnProperty(propertyName)) {
                            delete s[propertyName];
                        }
                    }

                    eventsSplitted = s.events.split(',');
                    for (var chunkStr in eventsSplitted) {
                        if(eventsSplitted[chunkStr] != 'event4') {
                            rcStr.push(eventsSplitted[chunkStr]);
                        }
                    }
                    s.events = rcStr.join(',');
                }
            }

