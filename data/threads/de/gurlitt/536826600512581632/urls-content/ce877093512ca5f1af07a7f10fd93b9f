 omnitureService = {
 s : null,
 init : function () {
 this.s = s_gi('nzzch');
 return this;
 },
 parametrizeOmnitureBySkyBoxCounter : function (skyBoxElementCount) {
 this.s.prop17 = skyBoxElementCount;
 },
 parametrizeOmnitureBySkyboxArticleName : function(rawArticleName) {
 this.s.prop18 = this.transformArticleNameToUnderscore(rawArticleName);
 },
 transformArticleNameToUnderscore : function (rawArticleName) {
 var cleanRawArticleName = rawArticleName.replace(/(\s){2,}/, ' ');
 var articleNameArray = cleanRawArticleName.split(' ');
 var articleNameArrayLwrCase = [];
 for(var articleNameIndex in articleNameArray){
 articleNameArrayLwrCase.push(articleNameArray[articleNameIndex].toLowerCase());
 }
 return articleNameArrayLwrCase.join('_');
 },
 parametrizeOmniture : function(socialIdentifier) {
 this.s.linkTrackVars = 'eVar21';
 this.s.eVar21 = socialIdentifier;
 this.s.tl(this,'o', socialIdentifier);
 },
 parametrizeOmnitureByStandardTeaser : function (clickedObject) {
 this.parametrizeOmnitureByTeaser(clickedObject, function(clickedObject, href, context) {
 var hrefParsed = context.getParsedHref(href);
 return hrefParsed.pageName + '|' + hrefParsed.guid + '|' + href;
 });
 },
 parametrizeOmnitureByTabTeaser : function(clickedObject) {
 this.parametrizeOmnitureByTeaser(clickedObject, function(clickedObject, href, context) {
 var tab = $(clickedObject).parents('section.teaser').find('ul.tabs li.ui-state-active a');
 var tabNumber = tab.attr('data-tab-number');
 var hrefParsed = context.getParsedHref(href);

 return hrefParsed.pageName + '|' + hrefParsed.guid + '|Tab' + tabNumber + '|' + href;
 });
 },
 parametrizeOmnitureByTeaser : function(clickedObject, parseStrFunc) {
 this.s.linkTrackVars = 'eVar17';
 var href = clickedObject.getAttribute('href');
 var evar17Fragment;
 
 if(typeof parseStrFunc == "function") {
 evar17Fragment = parseStrFunc(clickedObject, href, this);
 }
 this.s.eVar17 = evar17Fragment;
 this.s.tl(this,'o',evar17Fragment);
 document.location.href = href;
 },
 getParsedHref : function(href) {
 var hrefArray = href.split('/');
 var pageName = hrefArray.pop();
 var pageNameArray = pageName.split('-');
 var guid = pageNameArray.pop();

 return {'pageName': pageName, 'guid': guid};
 }
 }

