		jQuery('#headerSearch').focus(function (){
	    if (jQuery(this).val() == jQuery(this).attr("title")) {
		    jQuery(this).val('').addClass('searchFocus');
;
	    }
    }).blur(function () {
	    if (jQuery(this).val() == '') {
		    jQuery(this).val(jQuery(this).attr("title")).removeClass('searchFocus');
	  }
