				for (var i = 0; i < data.length; i++) {	

  				// javascript find by <li id="msgId_26949" class="">
          if ( $('#msgId_' + data[i].id).length == 0 ) {
            newMsgId = data[i].id;
          }

					// remove all modified (? all is an all) messages...
					if ($('#msgId_' + data[i].id).length) {	 
						$('#msgId_' + data[i].id).remove();
					}	
				}	
				for (var i = 0; i < data.length; i++) {
						if (data[i].deleted == 'false') {
						// insert message	
							$('#liveticker').prepend(data[i].html);	
						
						  if( newMsgId == data[i].id) {
						    $('#msgId_' + data[i].id).css('display', 'none');
